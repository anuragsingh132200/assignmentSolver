{
  "name": "assignment-agent-frontend",
  "version": "1.0.0",
  "description": "Frontend for Assignment Assistant Agent",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.3.2",
    "@types/node": "20.9.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "tailwindcss": "3.3.5",
    "autoprefixer": "10.4.16",
    "postcss": "8.4.31",
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.0.18",
    "react-hook-form": "7.47.0",
    "@hookform/resolvers": "3.3.2",
    "zod": "3.22.4",
    "axios": "1.6.0",
    "react-query": "3.39.3",
    "react-dropzone": "14.2.3",
    "react-markdown": "9.0.1",
    "react-syntax-highlighter": "15.5.0",
    "@types/react-syntax-highlighter": "15.5.11",
    "date-fns": "2.30.0",
    "clsx": "2.0.0",
    "lucide-react": "0.292.0"
  },
  "devDependencies": {
    "eslint": "8.53.0",
    "eslint-config-next": "14.0.3",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "prettier": "3.0.3",
    "prettier-plugin-tailwindcss": "0.5.7",
    "husky": "8.0.3",
    "lint-staged": "15.0.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
